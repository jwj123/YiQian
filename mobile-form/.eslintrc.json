{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "rules": {
        "no-new": "warn",
        "import/no-unresolved": "off", // 有的时候会在webpack中配置解析路径方便引用
        "import/first": "off",
        "import/no-extraneous-dependencies": "off",
        "import/extensions": "off",
        "no-param-reassign": "off",
        "import/prefer-default-export": "off",
        "no-underscore-dangle": "off"
    }
}