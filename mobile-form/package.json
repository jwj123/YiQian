{
  "name": "mobile-form",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "webpack-dev-server --config ./build/webpack.dev.config.js",
    "clean": "rm -rd ./public/js/**",
    "build:dev": "npm run clean; webpack --config ./build/webpack.dev.config.js && cp ./public/public.html ./views",
    "build:analyz": "npm run clean;  webpack --config ./build/webpack.analyz.js",
    "build:prod": "npm run clean; webpack --optimize-minimize --config ./build/webpack.prod.config.js  && cp ./public/public.html ./views"
  },
